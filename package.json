{
  "name": "lecashbot",
  "version": "1.5.0",
  "description": "A Discord.js economy bot based on the NitroType currency made with TypeScript. ",
  "main": "./src/index.js",
  "scripts": {
    "start": "npm run build && node --max-old-space-size=8192 ./build/index.js",
    "start:dev": "nodemon --exec babel-node ./src/index.js",
    "lint:dev": "eslint . --fix",
    "build": "npm run clean && npm run build-babel",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel ./src --out-dir ./build",
    "test": "jest ./src --passWithNoTests"
  },
  "keywords": [],
  "author": "ryan-harrington",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "axios": "^0.21.1",
    "chalk": "^4.0.0",
    "discord.js": "^12.4.1",
    "dotenv": "^10.0.0",
    "format-duration": "^1.3.1",
    "moment": "^2.24.0",
    "mongodb": "^3.5.9",
    "mongoose": "^5.12.11",
    "node-fetch": "^2.6.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@babel/node": "^7.14.2",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.14.2",
    "@babel/runtime": "^7.14.0",
    "eslint": "^7.27.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "jest": "^25.2.4",
    "nodemon": "^2.0.3"
  }
}