{
  "name": "lecashbot",
  "version": "3.0.0",
  "description": "A Discord.js economy bot based on the NitroType currency made with TypeScript.",
  "main": "index.ts",
  "scripts": {
    "build": "tsc",
    "dist": "node ./build/src/index.js",
    "prod": "pm2 start ./build/src/1index.js",
    "dev": "ts-node-dev --ignore-watch data ./src/index.ts",
    "dev:lint": "eslint . --fix --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheTypingMatch/lecashbot.git"
  },
  "keywords": [
