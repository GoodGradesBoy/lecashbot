    "discord.js",
    "nitrotype"
  ],
  "author": "TheTypingMatch",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TheTypingMatch/lecashbot/issues"
  },
  "homepage": "https://github.com/TheTypingMatch/lecashbot#readme",
  "dependencies": {
    "discord.js": "^12.5.2",
    "dotenv": "^8.2.0",
    "mongoose": "^5.12.3"
  },
  "devDependencies": {
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "eslint": "^7.23.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.3"
  }
